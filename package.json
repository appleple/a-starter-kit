{
  "name": "name",
  "version": "1.0.0",
  "description": "Webサイト制作プロジェクト",
  "keywords": [
    "CMS"
  ],
  "config": {
    "local": "http://example.com/",
    "theme": "root"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "node ./tools/eslint.js",
    "js:watch": "onchange \"themes/$npm_package_config_theme/js/src\" -- npm run js:build",
    "js:build": "npm run lint && browserify themes/$npm_package_config_theme/js/src/index.js -t babelify | uglifyjs -c > themes/$npm_package_config_theme/js/dest/bundle.js",
    "sync": "browser-sync start --proxy $npm_package_config_local --files ./themes/$npm_package_config_theme/** --open $npm_package_config_local",
    "css:watch": "gulp",
    "css:build": "gulp acmssass && gulp acmsmin && gulp sass",
    "start": "npm-run-all -p js:watch css:watch sync",
    "build": "npm-run-all js:build css:build"
  },
  "author": "appleple",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "browserify": "^13.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-csscomb": "^3.0.8",
    "gulp-cssnano": "^2.1.2",
    "gulp-gzip": "^1.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "node-notifier": "^5.1.2",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "uglifyjs": "^2.4.11"
  },
  "dependencies": {
    "awesome-sass-easing": "^1.1.2",
    "google-maps": "^3.2.1",
    "underscore": "^1.8.3",
    "url-dispatcher": "^0.1.5"
  }
}
