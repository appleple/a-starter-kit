{
  "name": "name",
  "version": "1.0.0",
  "description": "Webサイト制作プロジェクト",
  "keywords": [
    "CMS"
  ],
  "config": {
    "local": "http://example.com/",
    "theme": "root"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js:watch": "webpack --watch --config webpack.config.js --mode development",
    "js:build": "webpack -p --config webpack.config.js --mode production",
    "sync": "browser-sync start --proxy $npm_package_config_local --files ./themes/$npm_package_config_theme/** --open $npm_package_config_local",
    "css:watch": "gulp",
    "css:build": "gulp sass",
    "start": "npm-run-all -p js:watch css:watch sync",
    "build": "npm-run-all js:build css:build"
  },
  "author": "appleple",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "7",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-dead-code-elimination": "^2.2.2",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-html-import-to-string": "^0.0.1",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.8",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-csscomb": "^3.0.8",
    "gulp-cssnano": "^2.1.2",
    "gulp-gzip": "^1.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "node-notifier": "^5.1.2",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "uglify-js": "^3.0.8",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
